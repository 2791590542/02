int max(int x)
{
        int y = 0;
        for (y = 2; y < x; y++)
        {
               if (x%y == 0)
                       return 0;
        }
        return 1;
}
int main()
{
        int a = 0;
        scanf("%d", &a);
        if (max(a) == 1)
               printf("%d\n", a);
        else
               printf("0000\n");
}

