#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
int main()
{
        int i = 0;
        int n = 0;
        int m = 1;
        int t = 0;
        for (n=1; n <= 3; n++)
        {
               for (i=1;i<=n;i++)
               {
                       m = m * n;
               }
                       t = t + m;
        }
        printf("%d\n", t);
        return 0;
}
